{
  "name": "radio-api",
  "version": "1.5.1",
  "description": "REST API for accessing public data about a radio station.",
  "private": true,
  "engines": {
    "node": "8.11"
  },
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "test": "mocha --exit"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WHRB/radio-api"
  },
  "keywords": [
    "radio",
    "api"
  ],
  "author": "Carl Pillot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WHRB/radio-api/issues"
  },
  "homepage": "https://github.com/WHRB/radio-api",
  "dependencies": {
    "bluebird": "^3.5.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "logfmt": "^1.3.2",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "node-cache": "^4.2.0",
    "node-fetch": "^1.6.0",
    "superagent": "^5.1.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "husky": "^3.0.0",
    "lint-staged": "^9.2.0",
    "mocha": "^6.1.4",
    "nock": "^10.0.6",
    "prettier": "1.18.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
