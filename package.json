{
  "name": "radio-api",
  "version": "1.3.1",
  "description": "REST API for accessing public data about a radio station.",
  "private": true,
  "engines": {
    "node": "8.11"
  },
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "test": "mocha --exit",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WHRB/radio-api"
  },
  "keywords": [
    "radio",
    "api"
  ],
  "author": "Carl Pillot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WHRB/radio-api/issues"
  },
  "homepage": "https://github.com/WHRB/radio-api",
  "dependencies": {
    "bluebird": "^3.5.3",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "logfmt": "^1.2.1",
    "md5": "^2.2.1",
    "moment": "^2.23.0",
    "moment-timezone": "^0.5.23",
    "node-cache": "^4.2.0",
    "node-fetch": "^1.6.0",
    "superagent": "^3.8.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.10.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.1",
    "mocha": "^5.2.0",
    "nock": "^9.6.1",
    "prettier": "1.10.2"
  }
}
